{
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["warn",
        {
            "endOfLine": "auto"
        }
    ],
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off"
    },
    "parserOptions": {
        "project": "./tsconfig.json"
    }
}
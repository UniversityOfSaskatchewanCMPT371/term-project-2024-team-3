    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.6.5",
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-cookie": "^7.0.1",
